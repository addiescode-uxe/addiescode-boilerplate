    "@types/redux-logger": "^3.0.8",
    "@types/styled-components": "^5.1.3",
    "craco-alias": "^2.1.1",
    "dotenv-cli": "^4.0.0",
    "react-is": "^16.13.1",
    "webpack-spritesmith": "^1.1.0"
  }
