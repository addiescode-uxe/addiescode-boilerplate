    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "@typescript-eslint/parser": "^4.6.0",
    "antd": "^4.6.6",
    "antd-dayjs-webpack-plugin": "^1.0.1",
    "craco-less": "^1.17.0",
    "dayjs": "^1.9.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-hook-form": "^6.9.0",
    "react-redux": "^7.2.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.3",
    "redux": "^4.0.5",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "styled-components": "^5.2.0",
    "typescript": "^3.7.5",
    "webpack-spritesmith": "^1.1.0"
  },
  "scripts": {
    "start:develop": "dotenv -e .env.develop && craco start",
    "start:product": "dotenv -e .env.product && craco start",
    "build:develop": "dotenv -e .env.develop && craco build",
    "build:product": "dotenv -e .env.product && craco build",
    "deploy:develop": "dotenv -e .env.develop craco build && gulp deploy:develop",
    "deploy:product": "dotenv -e .env.product craco build && gulp deploy:product",
    "start": "craco start",
    "build": "craco build",
    "test": "craco test",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@storybook/addon-actions": "^6.0.26",
    "@storybook/addon-controls": "^6.0.26",
